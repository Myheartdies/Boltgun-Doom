// ========================================================================
// For player sound
// ========================================================================
// $playersound      sternguard	male	*pain100	STNPAIN1
// $playersounddup   sternguard	male	*pain100	STNPAIN3
// $playersounddup   sternguard	male	*pain75		*pain100
// $playersounddup   sternguard	male	*pain50		*pain100
// $playersounddup   sternguard	male	*pain25		*pain100

$playeralias sternguard gender *pain100 sternguard/pain
$playeralias sternguard gender *pain75 *pain100
$playeralias sternguard gender *pain50 *pain100
$playeralias sternguard gender *pain25 *pain100


// pain sound
$random sternguard/pain {sternguard/pain1 sternguard/pain2 sternguard/pain3 sternguard/pain4 
sternguard/pain5 sternguard/pain6 sternguard/pain7 sternguard/pain8 sternguard/pain9}
sternguard/pain1 STNPAIN1
sternguard/pain2 STNPAIN2
sternguard/pain3 STNPAIN3
sternguard/pain4 STNPAIN4
sternguard/pain5 STNPAIN5
sternguard/pain6 STNPAIN6
sternguard/pain7 STNPAIN7
sternguard/pain8 STNPAIN8
sternguard/pain9 STNPAIN9

// landing
$playeralias sternguard gender *grunt sternguard/landing
sternguard/landing_heavy STNLNDH1
$volume *land 0.3

$playeralias sternguard gender *land sternguard/landing
sternguard/landing STNLNDL1
$volume *land 0.25

// use fail
$playeralias sternguard gender *usefail sternguard/mumble
$random sternguard/mumble {sternguard/mumble1 sternguard/mumble2}
sternguard/mumble1 STNMUMB0
sternguard/mumble2 STNMUMB1

$volume *usefail 0.4


// Death sound
$playeralias sternguard gender *death sternguard/death
$playeralias sternguard gender *xdeath sternguard/death
$random sternguard/death {sternguard/death1 sternguard/death2 sternguard/death3 
sternguard/death4 sternguard/death5 sternguard/death6 sternguard/death7}
